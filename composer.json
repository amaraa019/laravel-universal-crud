{
  "name": "amaraa019/laravel-universal-crud",
  "description": "Laravel Breeze & React хослолд зориулсан Shadcn/ui + Laravel paginated response-оор ажилладаг Universal CRUD generator",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel","breeze", "react", "crud", "datatable", "shadcn", "inertia","vite","tailwindcss"],
  "authors": [
    {
      "name": "Амартүвшин",
      "email": "amaraa019@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Amaraa019\\UniversalCrud\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Amaraa019\\UniversalCrud\\UniversalCrudProvider"
      ]
    }
  }
}